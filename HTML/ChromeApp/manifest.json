{
    "manifest_version": 2,

    "name": "Oplop",
    "description": "A web app for creating consistent account passwords from nicknames and a master password.",
    "version": "2013.8.21.0",

    "icons": {
        "16": "assets/icons/icon016.png",
        "128": "assets/icons/icon128.png"
    },

    "app": {
        "launch": {
            "local_path": "index.html"
        }
    },

    "permissions": ["clipboardWrite", "storage"]
}
